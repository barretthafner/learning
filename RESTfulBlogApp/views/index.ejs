<% include ./partials/header %>

<h1>This is the index</h1>

<% blogs.forEach(function(blog){ %>
    <h3>
        <%= blog.title %>
    </h3>
    <img src="<%= blog.image %>">
    <span><%= blog.created %></span>
    <p><a href="/blogs/<%= blog._id %>">Read More</a></p>
<% }); %>

<% include ./partials/footer %>